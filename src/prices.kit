<!DOCTYPE html>
<html lang="uk">
  <!--@import partials/layout/head.html-->
  <body>
    <div class="page">
      <!-- #region header -->
      <!--@import partials/layout/header.html-->
      <!-- #endregion header -->

      <!-- #region sidebar -->
      <!--@import partials/layout/sidebar.html-->
      <!-- #endregion sidebar -->

      <!-- #region menu -->
      <!--@import partials/components/menu.html-->
      <!-- #endregion menu -->

      <!-- #region content -->
      <main class="content page__content">
        <div class="content__crumbs">
          <div class="container container--screen-xl">
            <ul class="crumbs">
              <li class="crumbs__item">
                <a class="crumbs__endpoint crumbs__endpoint--link" href="#">Головна</a>
              </li>
              <li class="crumbs__item">
                <span class="crumbs__endpoint">Ціни</span>
              </li>
            </ul>
          </div>
        </div>

        <!-- #region prices -->
        <section class="block content__block">
          <div class="container container--screen-xl">
            <header class="block__header">
              <h1 class="block__title h2">Ціни</h1>
            </header>

            <!-- #region tabs -->
            <div class="tabs base-tabs">
              <!-- #region tabs head -->
              <div class="tabs-head base-tabs__head">
                <button class="tabs-btn base-tabs__btn active" data-tab-target="kiyv">
                  Клініка в Києві
                </button>
                <button class="tabs-btn base-tabs__btn" data-tab-target="irpin">
                  Клініка в Ірпіні
                </button>
              </div>
              <!-- #endregion tabs head -->

              <!-- #region tabs content -->
              <div class="tabs-content base-tabs__content">
                <!-- #region tabs content item -->
                <div
                  class="tabs-content-item base-tabs__content-item"
                  data-tab-content="kiyv"
                >
                  <!--@import partials/components/price-accordion.html-->
                </div>
                <!-- #endregion tabs content item -->

                <!-- #region tabs content item -->
                <div
                  class="tabs-content-item base-tabs__content-item"
                  data-tab-content="irpin"
                >
                  <!--@import partials/components/price-accordion.html-->
                </div>
                <!-- #endregion tabs content item -->
              </div>
              <!-- #endregion tabs content -->
            </div>
            <!-- #endregion tabs -->
          </div>
        </section>
        <!-- #endregion doctors -->

        <!-- #region banner -->
        <section class="block content__block content__block--mb-0">
          <!--@import partials/components/banner-bottom-1.html-->
        </section>
        <!-- #endregion banner -->
      </main>
      <!-- #endregion content -->

      <!-- #region footer -->
      <!--@import partials/layout/footer.html-->
      <!-- #endregion footer -->
    </div>

    <!--@import partials/components/modal-appointment.html-->
    <!--@import partials/components/modal-success.html-->
    <!--@import partials/layout/scripts.html-->
